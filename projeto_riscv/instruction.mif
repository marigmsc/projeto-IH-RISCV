CONTENT					-- Start of (address: data pairs)
BEGIN

000: 01000000;		-- halt
001: 00000000;
002: 00000000;
003: 00000000;

004: 10110011;		-- xor x3,x3,x3
005: 11000001;
006: 00110001;
007: 00000000;

008: 00000011;		-- lw x2,1(x0)
009: 00100001;
010: 00010000;
011: 00000000;

END;